{
  "explorer.confirmDelete": false,
  "editor.rulers": [80, 120],
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "search.smartCase": true,
  "markdown.extension.print.absoluteImgPath": false,
  "workbench.settings.openDefaultSettings": true,
  "workbench.settings.useSplitJSON": true,
  "update.showReleaseNotes": false,
  "workbench.tips.enabled": false,
  "workbench.view.alwaysShowHeaderActions": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.iconTheme": "vscode-icons",
  "editor.renderWhitespace": "boundary",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "workbench.colorTheme": "One Dark Pro",
  "workbench.startupEditor": "none",
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": ["--max-line-length=120", "--max-doc-length=120", "--extend-ignore=F401"],
  "latex-workshop.latex.autoBuild.run": "never",
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.latex.tools": [
    {
      "name": "xelatex",
      "command": "xelatex",
      "args": ["-shell-escape", "-synctex=1", "-interaction=nonstopmode", "-file-line-error", "%DOC%"]
    }
  ],
  "latex-workshop.latex.recipes": [
    {
      "name": "xelatex",
      "tools": ["xelatex"]
    },
    {
      "name": "xelatex * 2",
      "tools": ["xelatex", "xelatex"]
    }
  ],
  "latex-workshop.latex.magic.args": [
    "-shell-escape",
    "-synctex=1",
    "-interaction=nonstopmode",
    "-file-line-error",
    "%DOC%"
  ],
  "python.analysis.memory.keepLibraryAst": true,
  "python.formatting.autopep8Args": ["--max-line-length", "120"],
  "files.trimTrailingWhitespace": true,
  "window.menuBarVisibility": "hidden",
  "editor.wordWrap": "on",
  "html.format.extraLiners": "",
  "html.format.indentHandlebars": true,
  "html.format.wrapAttributes": "aligned-multiple",
  "editor.minimap.enabled": false,
  "files.exclude": {
    "**/.idea": true
  },
  "files.watcherExclude": {
    "**/venv/**": true
  },
  "editor.fontLigatures": true,
  "vsicons.dontShowNewVersionMessage": true,
  "editor.fontFamily": "'Source Code Pro', 'Noto Sans CJK SC', 'Courier New', monospace",
  "editor.fontSize": 12,
  "http.proxy": "http://127.0.0.1:10800",
  "files.eol": "\n",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb",
    "*.hex": "hexEditor.hexEdit"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.configPath": "/home/myl/.prettierrc",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "restructuredtext.languageServer.disabled": true,
  "verilog.linting.linter": "verilator",
  "security.workspace.trust.enabled": false,
  "aws.telemetry": false,
  "redhat.telemetry.enabled": false,
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "github.copilot.enable": {
    "*": true
  }
}
