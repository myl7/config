{
  "explorer.confirmDelete": false,
  "editor.rulers": [80, 120],
  "search.smartCase": true,
  "markdown.extension.print.absoluteImgPath": false,
  "workbench.settings.openDefaultSettings": true,
  "workbench.settings.useSplitJSON": true,
  "update.showReleaseNotes": false,
  "workbench.tips.enabled": false,
  "workbench.view.alwaysShowHeaderActions": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.iconTheme": "vscode-icons",
  "editor.renderWhitespace": "boundary",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "workbench.colorTheme": "One Dark Pro",
  "workbench.startupEditor": "none",
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": [
    "--max-line-length=120",
    "--max-doc-length=120",
    "--extend-ignore=F401,E741"
  ],
  "latex-workshop.latex.autoBuild.run": "never",
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.latex.tools": [
    {
      "name": "xelatex",
      "command": "xelatex",
      "args": [
        "-shell-escape",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ]
    }
  ],
  "latex-workshop.latex.recipes": [
    {
      "name": "xelatex",
      "tools": ["xelatex"]
    },
    {
      "name": "xelatex * 2",
      "tools": ["xelatex", "xelatex"]
    }
  ],
  "latex-workshop.latex.magic.args": [
    "-shell-escape",
    "-synctex=1",
    "-interaction=nonstopmode",
    "-file-line-error",
    "%DOC%"
  ],
  "python.formatting.autopep8Args": ["--max-line-length", "120"],
  "files.trimTrailingWhitespace": true,
  "window.menuBarVisibility": "hidden",
  "editor.wordWrap": "on",
  "editor.minimap.enabled": false,
  "files.exclude": {
    "**/.idea": true
  },
  "files.watcherExclude": {
    "**/build/*/**": true,
    "**/target/*/**": true,
    "**/venv/**": true
  },
  "editor.fontLigatures": true,
  "vsicons.dontShowNewVersionMessage": true,
  "editor.fontFamily": "'DM Mono', 'Noto Sans CJK SC', monospace",
  "editor.fontSize": 12,
  "http.proxy": "http://127.0.0.1:10800",
  "files.eol": "\n",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb",
    "*.hex": "hexEditor.hexEdit"
  },
  "prettier.configPath": "/home/myl/.prettierrc.yaml",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "restructuredtext.languageServer.disabled": true,
  "verilog.linting.linter": "verilator",
  "security.workspace.trust.enabled": false,
  "aws.telemetry": false,
  "redhat.telemetry.enabled": false,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": false,
    "go": false
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "leetcode.workspaceFolder": "/home/myl/code/leetcode",
  "leetcode.defaultLanguage": "python3",
  "leetcode.hint.configWebviewMarkdown": false,
  "leetcode.hint.commentDescription": false,
  "leetcode.hint.commandShortcut": false,
  "terminal.integrated.localEchoExcludePrograms": [
    "vim",
    "vi",
    "nano",
    "tmux",
    "nvim"
  ]
}
